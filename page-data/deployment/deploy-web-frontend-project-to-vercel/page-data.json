childImageSharp":{"gatsbyImageData":{"layout":"constrained","placeholder":{"fallback":"data:image/jpeg;base64,"},"images":{"fallback":{"src":"/static/7455fde03fb5227d9df002dd1a95a6bd/f93c8/thumbnail.jpg","srcSet":"/static/7455fde03fb5227d9df002dd1a95a6bd/dc48f/thumbnail.jpg 205w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/60ce1/thumbnail.jpg 410w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/f93c8/thumbnail.jpg 820w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/53a8a/thumbnail.jpg 1640w","sizes":"(min-width: 820px) 820px, 100vw"},"sources":[{"srcSet":"/static/7455fde03fb5227d9df002dd1a95a6bd/23fb3/thumbnail.webp 205w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/14dc7/thumbnail.webp 410w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/a5d93/thumbnail.webp 820w,\n/static/7455fde03fb5227d9df002dd1a95a6bd/18a8b/thumbnail.webp 1640w","type":"image/webp","sizes":"(min-width: 820px) 820px, 100vw"}]},"width":820,"height":410}}}}}}]}},"pageContext":{"slug":"/deployment/deploy-web-frontend-project-to-vercel/","category":"Deployment"}},"staticQueryHashes":["1663765485","3636097453","4290078607"],"slicesMap":{}}